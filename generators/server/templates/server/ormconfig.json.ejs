[
  {
    "name": "default",
    "type": "sqlite",
    "database": "../target/db/sqlite-dev-db.sql",
    "synchronize": true,
    "logging": false,
    "entities": ["src/domain/*.model{.ts,.js}"]
  },
  {
    "name": "mongodb",
    "type": "mongodb",
    "url": "mongodb://localhost:27017/<%= baseName %>",
    "synchronize": true,
    "logging": false,
    "entities": ["src/domain/*.model{.ts,.js}"]
  },
  {
    "name": "mysql",
    "type": "mysql",
    "host": "localhost",
    "port": 3306,
    "username": "<%= baseName %>",
    "password": "<%= baseName %>",
    "database": "<%= baseName %>",
    "synchronize": true,
    "logging": false,
    "entities": ["src/domain/*.model{.ts,.js}"]
  }
]
